<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.text_field :title %>
  <%= f.text_field :artist_name %>
  <%= collection_select :genre_id, Genre.all, :id, :name %>
  <br />

  <%= form_for @song do |f| %>
  <input name="song[note_content][]' id='note_content_1'">
  <input name="song[note_content][]">
  <input name="song[note_content][]">
  <input type="submit" value="Submit">
<% end %>

  <%= f.submit %>
<% end %>
